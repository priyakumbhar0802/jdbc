<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EntryServlet</servlet-name>
  	<display-name>EntryServlet</display-name>
  	<description></description>
  	<servlet-class>com.controller.EntryServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>anu</servlet-name>
  	<display-name>anu</display-name>
  	<description></description>
  	<servlet-class>com.mypack.anu</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EntryServlet</servlet-name>
  	<url-pattern>/EntryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>anu</servlet-name>
  	<url-pattern>/anu</url-pattern>
  </servlet-mapping>
</web-app>
